{
  "name": "association-planning",
  "version": "1.0.0",
  "description": "Weekly planning management for association",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others-on-fail --kill-others \"nodemon server.js\" \"vite\"",
    "dev:server": "nodemon server.js",
    "dev:svelte": "vite",
    "build": "vite build",
    "seed": "node seed-real.js",
    "cleanup": "lsof -ti:3001 | xargs kill -9 || true"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "svelte-spa-router": "^4.0.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.32",
    "svelte": "^4.2.20",
    "tailwindcss": "^3.4.0",
    "vite": "^5.4.19"
  }
}
