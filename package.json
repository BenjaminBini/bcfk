    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "svelte-spa-router": "^4.0.1",
    "tailwind-scrollbar": "^3.1.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "tailwindcss": "^3.4.0"
  }
